{
  "compilerOptions": {
    "sourceMap": false,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "module": "amd",
    "moduleResolution": "node",
    "target": "es6",
    "declaration": true,
    "outDir": "out",
    "types": [ "vexflow" ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/tests",
    "node_modules"
  ]
}